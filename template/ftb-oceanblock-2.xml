<?xml version="1.0"?>
<Container>
  <Name>ftb OceanBlock</Name>
  <Repository>tr0mb4s/ftb-oceanblock-2:latest</Repository>
  <Registry>https://hub.docker.com/r/tr0mb4s/ftb-oceanblock-2</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/FTB-Dockers/ftb-oceanblock-2//issues</Support>
  <Project>https://github.com/FTB-Dockers/ftb-oceanblock-2</Project>
  <Overview>This is a pre-installed Feed The Beast OceanBlock Minecraft server (modpack ID 128).</Overview>
  <Category>GameServers:Minecraft</Category>
  <Icon>https://raw.githubusercontent.com/FTB-Dockers/ftb-oceanblock-2/blob/main/icons/ftb-oceanblock-2.png</Icon>

  <Config Name="Server Port" Target="25565" Default="25565" Mode="tcp" Description="Minecraft Server Port" Type="Port" />
  <Config Name="World Data" Target="/data" Default="/mnt/user/appdata/oceanblock" Mode="rw" Description="World data storage" Type="Path" />
  <Config Name="EULA" Target="EULA" Default="true" Description="Accept Minecraft EULA" Type="Variable" />
  <Config Name="Memory" Target="MEMORY" Default="6G" Description="Maximum server memory" Type="Variable" />
</Container>
